{
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "editor.accessibilitySupport": "off",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.fontSize": 16,
  "telemetry.telemetryLevel": "off",
  "files.watcherExclude": {
    "**/build/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "files.exclude": {
    "release-info": true,
    "src/*/build": false,
    "build": false,
    "env": true,
    "packageInfo*": true,
    ".bemol": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "search.exclude": {
    "env": true,
    "src/*/build": true,
    "build": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "editor.rulers": [80],
  "workbench.iconTheme": "material-icon-theme",
  "editor.wordBasedSuggestions": false,
  "rust-analyzer.procMacro.ignored": {},
  "rust-analyzer.completion.privateEditable.enable": true,
  "rust-analyzer.procMacro.enable": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "errorLens.statusBarMessageType": "closestSeverity",
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.messageMaxChars": 85,
  "errorLens.enabled": true,
  "cSpell.userWords": ["decompressor", "slicebuffer"],
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error"],
  "errorLens.messageBackgroundMode": "message",
  "hexeditor.columnWidth": 20,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "editor.inlayHints.fontSize": 11,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "recentlyUsed",
  "rust-analyzer.procMacro.attributes.enable": false,
  "editor.quickSuggestionsDelay": 60,
  "shellcheck.run": "onSave",
  "vsintellicode.modify.editor.suggestSelection": "disabled",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "editor.fontLigatures": false,
  "editor.inlayHints.enabled": true,
  "window.experimental.commandCenter": true,
  "errorLens.onSaveTimeout": 500,
  "terminal.integrated.fontSize": 14,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSaveDelay": 500,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.linux": "fish",
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.inlineSuggest.enabled": true,
  "cSpell.enabled": false,
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.panel.defaultLocation": "right",
  "vscode-neovim.neovimExecutablePaths.linux": "nvim",
  "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
  "terminal.integrated.altClickMovesCursor": false,
  "editor.fontFamily": "MesloLGS Nerd Font Mono,Menlo,DejaVu Serif,'Droid Sans Mono', 'monospace', monospace",
  "window.commandCenter": true,
  "window.titleBarStyle": "custom"
}
